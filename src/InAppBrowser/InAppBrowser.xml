<?xml version="1.0" encoding="utf-8" ?>
<widget id="InAppBrowser.widget.InAppBrowser" needsEntityContext="true" offlineCapable="true" xmlns="http://www.mendix.com/widget/1.0/">
    <name>InAppBrowser</name>
    <description>The description of this widget.</description>
    <icon></icon>

    <properties>
        <property key="urllink" type="attribute" required="true">
            <caption>URL</caption>
            <category>Datasource</category>
            <description>The URL that must be oppened.</description>
            <attributeTypes>
                <attributeType name="String" />
            </attributeTypes>
        </property>

        <property key="buttonText" type="translatableString" required="true">
            <caption>Link text</caption>
            <category>Appearance</category>
            <description>The text of the link</description>
            <translations>
                <translation lang="en_US">Go</translation>
                <translation lang="nl_NL">Ga</translation>
            </translations>
        </property>
        <property key="buttonIcon" type="string" defaultValue="glyphicon glyphicon-chevron-right">
            <caption>Link glyphicon</caption>
            <category>Appearance</category>
            <description>The complete classes to display the required</description>
        </property>
        <property key="buttonRender" type="enumeration" defaultValue="button">
            <caption>Link render</caption>
            <category>Appearance</category>
            <description>Must the link be rendered as button or link</description>
            <enumerationValues>
                <enumerationValue key="link">Link</enumerationValue>
                <enumerationValue key="button">Button</enumerationValue>
            </enumerationValues>
        </property>

        <property key="hideURLBar" type="boolean" defaultValue="true">
            <caption>Hide URL Bar</caption>
            <category>In App Browsing</category>
            <description>Hide the URL bar</description>
        </property>
        <property key="hideNavBtn" type="boolean" defaultValue="true">
            <caption>Hide navigation button</caption>
            <category>In App Browsing</category>
            <description>Hide the navigation (back and forward) buttons</description>
        </property>
        <property key="disallowScroll" type="boolean" defaultValue="true">
            <caption>Disallow over scroll</caption>
            <category>In App Browsing</category>
            <description>Prevent over scoll kike an extra pull down.</description>
        </property>
        <property key="closeText" type="translatableString" required="true">
            <caption>Close text</caption>
            <category>In App Browsing</category>
            <description>The text of the link/button in the In App Browser to close the page.</description>
            <translations>
                <translation lang="en_US">Close</translation>
                <translation lang="nl_NL">Sluiten</translation>
            </translations>
        </property>

    </properties>
</widget>

